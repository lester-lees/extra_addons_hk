<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
		<template id="report_sale_order_custom_hk_document">
			<t t-call="report.external_layout">
				<div t-if="o.partner_id.country_id.code != 'TW'" class="page">
				    <t t-set="all_brand_total" t-value="0"/>
					<div class="oe_structure"/>

					<h4>Invoice: <span t-field="o.sale_id.partner_id.ref"/>-<span t-field="o.sale_id.name"/></h4>					
			
					<!--   Invoice Address start   -->					
					 <table  class="dest_address" style="table-layout:fixed; width: 100%">			
						<tr><td width="20%"><strong>Date: </strong></td><td><span t-field="o.sale_id.date_confirm"/></td></tr>
						
						<tr><td width="20%"><strong>Invoice Consignee: </strong></td><td><span t-field="o.sale_id.partner_invoice_id.name"/>
						<t t-if="o.sale_id.partner_invoice_id.function">, <span t-field="o.sale_id.partner_invoice_id.function"/></t></td>
						</tr>
						
						<tr>
							<td width="20%"><strong>Invoice Address: </strong> </td>
							<td><span t-field="o.sale_id.partner_invoice_id.street"/><t t-if="not o.sale_id.partner_invoice_id.street"><span t-field="o.sale_id.partner_id.street"/>,<span t-field="o.sale_id.partner_id.street2"/></t> <span t-field="o.sale_id.partner_invoice_id.street2"/> , 
							<span t-field="o.sale_id.partner_invoice_id.city"/><t t-if="not o.sale_id.partner_invoice_id.city"><span t-field="o.sale_id.partner_id.city"/></t>, 								
							<span t-field="o.sale_id.partner_invoice_id.country_id.name"/><t t-if="not o.sale_id.partner_invoice_id.country_id.name"><span t-field="o.sale_id.partner_id.country_id.name"/></t>
							<t t-if="o.sale_id.partner_invoice_id.zip">, <strong>Zip:<span t-field="o.sale_id.partner_invoice_id.zip"/></strong></t>
							</td>
						</tr>
						
						<tr t-if="o.sale_id.partner_invoice_id.phone or o.sale_id.partner_id.phone"><td width="20%"><strong>Phone:</strong> </td>
						<td><span t-field="o.sale_id.partner_invoice_id.phone"/>
						<t t-if="not o.sale_id.partner_invoice_id.phone"><span t-field="o.sale_id.partner_id.phone"/></t></td>
						</tr>
						
						<tr t-if="o.sale_id.partner_invoice_id.mobile or o.sale_id.partner_id.mobile"><td width="20%"><strong>Mobile:</strong> </td>
						<td><span t-field="o.sale_id.partner_invoice_id.mobile"/>
						<t t-if="not o.sale_id.partner_invoice_id.mobile"><span t-field="o.sale_id.partner_id.mobile"/></t></td>
						</tr>
					 </table>

					<!--   Shipping Address start   -->						 
					 <table t-if="o.sale_id.partner_invoice_id != o.sale_id.partner_shipping_id"  class="dest_address" style="table-layout:fixed; width: 100%">								
						<tr><td width="20%"><strong>Shipping Consignee: </strong></td><td><span t-field="o.sale_id.partner_shipping_id"/>
						<t t-if="o.sale_id.partner_shipping_id.function">, <span t-field="o.sale_id.partner_shipping_id.function"/></t></td>
						</tr>
						
						<tr t-if="o.sale_id.partner_shipping_id.street and o.sale_id.partner_shipping_id.street != o.sale_id.partner_invoice_id.street">
							<td width="20%"><strong>Shipping Address: </strong> </td>
							<td><span t-field="o.sale_id.partner_shipping_id.street"/><t t-if="not o.sale_id.partner_shipping_id.street"><span t-field="o.sale_id.partner_id.street"/>,<span t-field="o.sale_id.partner_id.street2"/></t> <span t-field="o.sale_id.partner_shipping_id.street2"/> ,
							<span t-field="o.sale_id.partner_shipping_id.city"/><t t-if="not o.sale_id.partner_shipping_id.city"><span t-field="o.sale_id.partner_id.city"/></t>, 
									
							<span t-field="o.sale_id.partner_shipping_id.country_id.name"/><t t-if="not o.sale_id.partner_shipping_id.country_id.name"><span t-field="o.sale_id.partner_id.country_id.name"/></t>
							<t t-if="o.sale_id.partner_shipping_id.zip">, <strong>Zip:<span t-field="o.sale_id.partner_shipping_id.zip"/></strong></t>
							</td>
						</tr>
						<tr t-if="o.sale_id.partner_shipping_id.phone and o.sale_id.partner_shipping_id.phone != o.sale_id.partner_invoice_id.phone">
							<td width="20%"><strong>Phone:</strong> </td>
							<td><span t-field="o.sale_id.partner_shipping_id.phone"/>
							<t t-if="not o.sale_id.partner_shipping_id.phone"><span t-field="o.sale_id.partner_id.phone"/></t></td>
						</tr>
						
						<tr t-if="o.sale_id.partner_shipping_id.mobile and o.sale_id.partner_shipping_id.mobile != o.sale_id.partner_invoice_id.mobile"><td width="20%"><strong>Mobile:</strong> </td>
						<td><span t-field="o.sale_id.partner_shipping_id.mobile"/>
						<t t-if="not o.sale_id.partner_shipping_id.mobile"><span t-field="o.sale_id.partner_id.mobile"/></t></td>
						</tr>
					 </table>
					 
					<br/>
					<b>Dear Sir/Madam: </b><br/>
					<span>Thank you for your order! Please find the details bellow.</span><br/>
					<span>"kindly arrange payment to our account number:"</span><br/>
					<t t-foreach="o.company_id.bank_ids" t-as="bank">
						<span t-field="bank.acc_number"/>
						<span>at </span><span t-field="bank.bank_name"/>
						<span>SWIFT/IBNA: " </span><span t-field="bank.swift_ibna"/>" <br/>
					</t>
										
					<t t-set="all_brand_total" t-value="0"/>
					<t t-foreach="lines_group_by_brand_sample(order=o.sale_id,move_lines=o.move_lines,half_price=o.sale_id.partner_id.half_price).items()" t-as="cur_group">				
						<!-- lines in one currency-->
						<t t-set="p_currency" t-value="cur_group[1]['currency_name']"/>						
						<t t-if="cur_group[1]['type'] != 'Replacement' and cur_group[1]['type'] != 'RMA' and cur_group[1]['type'] != 'Sample' and cur_group[1]['type'] != 'Shortship' and cur_group[1]['type'] != 'Service' and cur_group[1]['qty_total'] != 0">
							<table class="table-condensed" width="100%">
								<thead>
									<tr>
										<th width="5%">POS</th>
										<th width="40%">Product Name</th>
										<th width="35%">Remarks</th>
										<th width="5%" class="text-right">Qty. </th>
										<th width="5%" class="text-right">Price</th>
										<th width="10%" class="text-right">Total</th>
									</tr>
								</thead>
								
								<t t-foreach="cur_group[1]['lines']" t-as="line">
									<tr t-if="line['product_uom_qty'] != 0">
										<td width="5%"><t t-esc="line['id'] or ' '"/></td>
										<td width="40%"><t t-esc="line['product_name'] or ' '"/></td>
										<td width="35%"><t t-esc="line['name'] or ' '"/></td>
										<td width="5%" class="text-right">
											<t t-esc="int(line['product_uom_qty'])"/>
										</td>
										<td width="5%" class="text-right">
											<t t-esc="formatLang(line['total_without_discount_in_order_cur']/line['product_uom_qty'], dp='Account')"/>
										</td>
										<td width="10%" class="text-right">
											<t t-esc="formatLang(line['total_without_discount_in_order_cur'], dp='Account')"/>
										</td>
									</tr>
								</t>					
							</table>						
							
							
							<div class="row">
								<div class="col-xs-4 pull-right">
									<table class="table table-condensed">
								   <!-- <tr class="border-black">
											<td width="90%"><strong>SubTotal(<t t-esc="cur_group[1]['currency_name']"/>) </strong></td>
											<td class="text-right">
												<span t-esc="cur_group[1]['subtotal']"/>
											</td>
										</tr>
										<tr t-if="cur_group[1]['discount_amount'] != 0" class="border-black">
											<td width="90%">Discount(<t t-esc="int(cur_group[1]['discount_rate'])"/>%)</td>
											<td class="text-right">
												<span t-esc="cur_group[1]['discount_amount']"/>
											</td>
										</tr>   -->
										<tr t-if="cur_group[1]['subtotal'] != cur_group[1]['subtotal_without_discount']" class="border-black">
											<td width="90%"><strong>SubTotal(<t t-esc="cur_group[1]['currency_name']"/>)</strong></td>
											<td class="text-right">
												<span t-esc="cur_group[1]['subtotal_without_discount']"/>
											</td>
										</tr>
										<tr t-if="o.sale_id.currency_id.name != cur_group[1]['currency_name']" class="border-black">
											<td width="90%"><strong>SubTotal(<span t-field="o.sale_id.currency_id.name"/>)</strong></td>
											<td class="text-right">
												<span t-esc="cur_group[1]['subtotal_in_order_currency']"/>
											</td>
										</tr>
										<t t-set="all_brand_total" t-value="all_brand_total + cur_group[1]['subtotal_in_order_currency']"/>										
									</table>
								</div>
							</div>
						</t>					
					</t>
					
					<!-- special promotion 
					<t t-foreach="lines_group_by_brand_sample(order=o.sale_id,move_lines=o.move_lines,half_price=o.sale_id.partner_id.half_price).items()" t-as="cur_group">		
						<t t-if="cur_group[1]['type'] == 'Promotion'">
							<h4>Special <t t-esc="cur_group[1]['type']"/></h4>
							<table class="table-condensed" width="100%">
								<thead>
									<tr>
										<th>POS</th>
										<th>Product Name</th>
										<th class="text-right">Qty. </th>
										<th class="text-right">Price</th>
										<th class="text-right">Amount</th>
										<th class="text-right" width="15%">Discount </th>
										<th class="text-right">After.Disc</th>
										<th class="text-right">Total(<span t-esc="o.sale_id.currency_id.name"/>)</th>
									</tr>
								</thead>
								
								<t t-foreach="cur_group[1]['lines']" t-as="line">
									<tr>
										<td><t t-esc="line['id'] or ' '"/></td>
										<td><t t-esc="line['product_name'] or ' '"/></td>
										<td class="text-right">
											<t t-esc="int(line['product_uom_qty'])"/>
										</td>
										<td class="text-right">
											<t t-esc="line['price_unit']"/>
										</td>
										<td class="text-right">
											<t t-esc="line['price_total']"/>(<t t-esc="line['currency_name']"/>)
										</td>
										<td class="text-right" width="15%">
											<t t-esc="int(line['discount_rate'])"/>%, <t t-esc="line['line_discount_amount']"/>
										</td>
										<td class="text-right">
											<t t-esc="round(line['price_total'] - line['line_discount_amount'],2)"/>(<t t-esc="line['currency_name']"/>)
										</td>										
										<td class="text-right">
											<t t-esc="formatLang(line['total_without_discount_in_order_cur'], dp='Account')"/>
										</td>										
									</tr>
								</t>
							</table>
							
							<div class="row">
								<div class="col-xs-3 pull-right">
									<table class="table table-condensed">
										<tr class="border-black">
											<td width="90%"><strong>SubTotal(<span t-field="o.sale_id.currency_id.name"/>)</strong></td>
											<td class="text-right">
												<span t-esc="cur_group[1]['subtotal_in_order_currency']"/>
												<t t-set="all_brand_total" t-value="all_brand_total + cur_group[1]['subtotal_in_order_currency']"/>
											</td>
										</tr>
									</table>
								</div>
							</div>
						</t>
					</t>	-->								
					
					<!--     samples  and shortships here -->
					<t t-foreach="lines_group_by_brand_sample(order=o.sale_id,move_lines=o.move_lines,half_price=o.sale_id.partner_id.half_price).items()" t-as="cur_group">				
						<!-- lines in one currency-->
						<t t-if="cur_group[1]['type'] == 'Replacement' or cur_group[1]['type'] == 'Sample' or cur_group[1]['type'] == 'Shortship' and cur_group[1]['type'] == 'RMA'">
							<h4><t t-esc="cur_group[1]['type']"/></h4>
							<table class="table-condensed" width="100%">
								<thead>
									<tr>
										<th width="5%">POS</th>
										<th width="40%">Product Name</th>
										<th width="35%">Remarks</th>
										<th width="5%" class="text-right">Qty. </th>
										<th width="5%" class="text-right">Price</th>
										<th width="10%" class="text-right">Total</th>
									</tr>
								</thead>
								<t t-set="sample_val" t-value="0"/>
								<t t-foreach="cur_group[1]['lines']" t-as="line">
									<tr>
										<td width="5%"><t t-esc="line['id'] or ' '"/></td>
										<td width="40%"><t t-esc="line['product_name'] or ' '"/></td>
										<td width="35%"><t t-esc="line['name'] or ' '"/></td>
										<td width="5%" class="text-right">
											<t t-esc="int(line['product_uom_qty'])"/>
										</td>
										<td width="5%" class="text-right">0.01</td>
										<td width="10%" class="text-right"><t t-esc="line['product_uom_qty']*0.01"/></td>
										<t t-set="sample_val" t-value="sample_val + line['product_uom_qty']*0.01"/>	
									</tr>
								</t>
								<t t-set="all_brand_total" t-value="all_brand_total + sample_val"/>								
							</table>
							<div class="row">
								<div class="col-xs-4 pull-right">
									<table class="table table-condensed">
										<tr class="border-black">
											<td width="90%"><strong>SubTotal(<span t-field="o.sale_id.currency_id.name"/>) </strong></td>
											<td class="text-right"><span t-esc="sample_val"/></td>
										</tr>
									</table>
								</div>
							</div>
                        </t>							
					</t>
					
					<br/><br/>
					<div class="row">
						<div class="col-xs-6 pull-right">
							<table class="table table-condensed">
								<tr class="border-black">
									<td width="90%"><strong>Total(<span t-field="o.sale_id.currency_id.name"/>)</strong></td>
									<td class="text-right">
										<span t-esc="all_brand_total"/>
									</td>
								</tr>

								<tr t-if="o.sale_id.user_id.name in ['anthony','ray']" class="border-black">
									<td width="90%"><strong>Rounding(<span t-field="o.sale_id.currency_id.name"/>)</strong></td>
									<td class="text-right">
										<span t-esc="rounding(all_brand_total)"/>
									</td>
								</tr>
							</table>
						</div>
					</div>
					
					<div><p><strong>Best regards, </strong><br/><strong t-field="o.sale_id.user_id.name"> </strong></p></div>
                    <div t-if="o.delaration"><strong>Remark:</strong><br/><p t-field="o.delaration"/></div>				
				</div>			

				<!--  ==============================Tai Wan Custom Invoice ============================================== -->
				
				<div t-if="o.partner_id.country_id.code == 'TW'" class="page">
				    <t t-set="all_brand_total" t-value="0"/>
					<div class="oe_structure"/>

					<h4>Invoice: <span t-field="o.sale_id.partner_id.ref"/>-<span t-field="o.sale_id.name"/></h4>					

					 <table  class="dest_address" style="table-layout:fixed; width: 100%">			
						<tr><td width="12%"><strong>Date: </strong></td><td><span t-field="o.sale_id.date_confirm"/></td></tr>
						
						<tr><td width="12%"><strong>Consignee: </strong></td><td><span t-field="o.sale_id.partner_invoice_id"/>
						<t t-if="o.sale_id.partner_invoice_id.function">, <span t-field="o.sale_id.partner_invoice_id.function"/></t></td>
						</tr>
						
						<tr><td width="12%"><strong>Address: </strong> </td>
						<td><span t-field="o.sale_id.partner_invoice_id.street"/><t t-if="not o.sale_id.partner_invoice_id.street"><span t-field="o.sale_id.partner_id.street"/>,<span t-field="o.sale_id.partner_id.street2"/></t> <span t-field="o.sale_id.partner_invoice_id.street2"/>
						</td>
						</tr>
						
						<tr><td width="12%"></td>
							<td>
								<span t-field="o.sale_id.partner_invoice_id.city"/> 
								<t t-if="not o.sale_id.partner_invoice_id.city"><span t-field="o.sale_id.partner_id.city"/></t>, 
								
								<span t-field="o.sale_id.partner_invoice_id.country_id.name"/>
								<t t-if="not o.sale_id.partner_invoice_id.country_id.name"><span t-field="o.sale_id.partner_id.country_id.name"/></t>
							</td>
						</tr>
						<tr><td width="12%"><strong>Phone:</strong> </td>
						<td><span t-field="o.sale_id.partner_invoice_id.phone"/>
						<t t-if="not o.sale_id.partner_invoice_id.phone"><span t-field="o.sale_id.partner_id.phone"/></t></td>
						</tr>
						
						<tr><td width="12%"><strong>Mobile:</strong> </td>
						<td><span t-field="o.sale_id.partner_invoice_id.mobile"/>
						<t t-if="not o.sale_id.partner_invoice_id.mobile"><span t-field="o.sale_id.partner_id.mobile"/></t></td>
						</tr>
					 </table>
					
					<br/>
					<b>Dear Sir/Madam: </b><br/>
					<span>Thank you for your order! Please find the details bellow.</span><br/>
					<span>"kindly arrange payment to our account number:"</span><br/>
					<t t-foreach="o.company_id.bank_ids" t-as="bank">
						<span t-field="bank.acc_number"/>
						<span>at </span><span t-field="bank.bank_name"/>
						<span>SWIFT/IBNA: " </span><span t-field="bank.swift_ibna"/>" <br/>
					</t>
										
				    <br/>
					<t t-set="all_brand_total" t-value="0"/>
